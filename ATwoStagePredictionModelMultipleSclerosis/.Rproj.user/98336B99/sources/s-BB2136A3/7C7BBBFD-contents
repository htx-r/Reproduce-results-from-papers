STUDY<-as.data.frame(cbind(c("GRIPHON (AC-065A302) (N=1152)", "TRITON (AC-065A308) (N=246)", "COMPASS-2 (AC-052-414) (N=334)", "SERAPHIN (AC-055-302)(N=742)")))
