library(readxl)
library(ggplot2)
GraphdataF<-read.csv("C:/Users/kc19o338/Documents/GitHub/shinies/apps/koms/Data_for_the_Graph7.csv")

IPDplotPreSpecified<-ggplot(GraphdataF, aes(x=Baseline.risk.score, y=Predicted.probability.to.relapse.in.2.years, group=Treatment)) +
  geom_line(aes(color=Treatment))+
  scale_color_manual(values=c("lightgoldenrod3", "seagreen3", "orange", "steelblue3"))+
  geom_point(aes(color=Treatment))+geom_vline(xintercept=0.05113*100, linetype="dashed", color = "red")+
  geom_vline(xintercept=0.72461*100, linetype="dashed", color = "red")+
  theme_classic()
IPDplotPreSpecified<-IPDplotPreSpecified+ xlab("Baseline risk") + ylab("Predicted probability of relapse")
